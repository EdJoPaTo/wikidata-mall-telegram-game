{
	"name": "wikidata-mall-telegram-game",
	"private": true,
	"version": "0.12.1",
	"description": "A Telegram Game where you can build your own mall based on Wikidata entities",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-mall-telegram-game",
	"author": {
		"name": "EdJoPaTo",
		"email": "edjopato@gmail.com",
		"url": "edjopato.de"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"start": "ts-node source/index.ts",
		"test": "xo && NODE_ICU_DATA='node_modules/full-icu' nyc ava"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"keywords": [],
	"dependencies": {
		"array-filter-unique": "^2.1.0",
		"emoji-regex": "^8.0.0",
		"full-icu": "^1.3.0",
		"gaussian": "^1.1.0",
		"json-stable-stringify": "^1.0.1",
		"locale-emoji": "^0.3.0",
		"node-schedule": "^1.3.2",
		"random-item": "^3.0.0",
		"source-map-support": "^0.5.13",
		"telegraf": "^3.30.1",
		"telegraf-i18n": "^6.5.0",
		"telegraf-inline-menu": "^3.6.2",
		"telegraf-session-local": "^1.0.2",
		"telegraf-wikibase": "^0.3.0",
		"telegram-format": "^1.1.0",
		"telegram-typings": "^3.6.1",
		"timezone-support": "^2.0.2",
		"wikidata-entity-reader": "^1.3.0",
		"wikidata-entity-store": "^1.1.0",
		"wikidata-sdk-got": "^1.1.1",
		"write-json-file": "^4.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.4.0",
		"@types/emoji-regex": "^8.0.0",
		"@types/gaussian": "^1.1.1",
		"@types/json-stable-stringify": "^1.0.32",
		"@types/node": "^12.0.2",
		"@types/node-schedule": "^1.2.3",
		"@typescript-eslint/eslint-plugin": "^1.10.2",
		"@typescript-eslint/parser": "^1.10.2",
		"ava": "^2.1.0",
		"del-cli": "^3.0.0",
		"eslint-config-xo-typescript": "^0.15.0",
		"nyc": "^14.1.1",
		"ts-node": "^8.0.2",
		"typescript": "^3.5.3",
		"xo": "^0.24.0"
	},
	"ava": {
		"babel": false,
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"source"
		],
		"exclude": [
			"**/*.test.ts"
		],
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"semi": false,
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"rules": {
			"ava/no-ignored-test-files": "off",
			"@typescript-eslint/array-type": "warn",
			"@typescript-eslint/semi": [
				"error",
				"never"
			]
		}
	}
}
