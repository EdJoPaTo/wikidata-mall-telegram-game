{
	"name": "wikidata-mall-telegram-game",
	"private": true,
	"version": "1.2.1",
	"description": "A Telegram Game where you can build your own mall based on Wikidata entities",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-mall-telegram-game",
	"author": {
		"name": "EdJoPaTo",
		"email": "wikidata-mall-telegram-game@edjopato.de",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"start": "ts-node source/index.ts",
		"test": "tsc && xo && NODE_ICU_DATA='node_modules/full-icu' nyc ava"
	},
	"main": "dist/source",
	"types": "dist/source",
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"keywords": [],
	"dependencies": {
		"@edjopato/datastore": "^0.3.0",
		"array-filter-unique": "^2.1.0",
		"array-reduce-group-by": "^1.0.0",
		"emoji-regex": "^9.0.0",
		"full-icu": "^1.3.0",
		"gaussian": "^1.1.0",
		"json-stable-stringify": "^1.0.1",
		"locale-emoji": "^0.3.0",
		"node-schedule": "^1.3.2",
		"random-item": "^3.0.0",
		"source-map-support": "^0.5.13",
		"telegraf": "^3.38.0",
		"telegraf-i18n": "^6.5.0",
		"telegraf-inline-menu": "^5.0.0",
		"telegraf-middleware-console-time": "^1.1.0",
		"telegraf-session-local": "^2.0.0",
		"telegraf-wikibase": "^0.9.0",
		"telegram-format": "^2.0.0",
		"timezone-support": "^2.0.2",
		"wikidata-entity-reader": "^2.0.0",
		"wikidata-person-names": "^1.0.0",
		"wikidata-sdk-got": "^2.0.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.8.0",
		"@types/emoji-regex": "^8.0.0",
		"@types/gaussian": "^1.1.1",
		"@types/json-stable-stringify": "^1.0.32",
		"@types/node": "^14.0.13",
		"@types/node-schedule": "^1.2.3",
		"ava": "^3.8.2",
		"del-cli": "^3.0.0",
		"nyc": "^15.0.0",
		"ts-node": "^9.0.0",
		"typegram": "^2.2.2",
		"typescript": "^4.1.2",
		"xo": "^0.35.0"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"dist/source",
			"source"
		],
		"exclude": [
			"**/*.test.ts"
		],
		"all": false,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"ava/no-ignored-test-files": "off",
			"require-atomic-updates": "off",
			"unicorn/no-reduce": "off",
			"@typescript-eslint/no-dynamic-delete": "off",
			"@typescript-eslint/no-var-requires": "warn",
			"@typescript-eslint/prefer-nullish-coalescing": "off",
			"@typescript-eslint/prefer-readonly-parameter-types": "off",
			"@typescript-eslint/restrict-template-expressions": "off"
		}
	}
}
